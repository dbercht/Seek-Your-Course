<div class="learning-results clearfix">
   	  <h1>RESULTS</h1>
  
          <% @offerings.each do |@offering| %>
             <%= render :partial => @offering %>
          <% end %>

<div class="syc_pagination">
  <%= will_paginate @offerings, :container => false %>
  <div class="page_info">
    <%= "(#{Offering.all.size.to_s} Listings)" unless params[:coordinated] %>
  </div>
</div>
</div>


<%= render :partial => 'offerings/browse' %>

<% content_for :filter_results do %>

  <h2> Filter Results </h2>
   <h3>Topic</h3>
    <%@topics.each do |topic| %>
      <%= link_to topic.category, (request.path + "?topic=" +topic.id.to_s) %>
    <% end %>
   <h3>Type</h3>
    <%@types.each do |type| %>
      <%= link_to type.category, (request.path + "?type=" +type.id.to_s) %>
    <% end %>

<% end %>
